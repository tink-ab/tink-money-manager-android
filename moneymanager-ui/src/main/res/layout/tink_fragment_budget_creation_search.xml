<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="?attr/tink_backgroundColor">

  <androidx.recyclerview.widget.RecyclerView
    android:id="@+id/searchSuggestionsList"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/tink_toolbar"
    app:layout_constraintBottom_toBottomOf="parent"
    tools:listitem="@layout/tink_item_budget_creation_search_suggestion"/>

  <com.tink.moneymanagerui.view.TinkToolbar
      android:id="@+id/tink_toolbar"
      android:layout_width="match_parent"
      android:layout_height="?actionBarSize"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:navigationContentDescription="@string/tink_nav_app_bar_navigate_up_description"
      app:navigationIcon="?android:homeAsUpIndicator" />

</androidx.constraintlayout.widget.ConstraintLayout>