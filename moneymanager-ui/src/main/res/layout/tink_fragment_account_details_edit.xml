<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="?tink_backgroundColor"
tools:ignore="ContentDescription">

<com.tink.moneymanagerui.view.TinkToolbar
  android:id="@+id/tink_toolbar"
  android:layout_width="match_parent"
  android:layout_height="?actionBarSize"
  app:layout_constraintTop_toTopOf="parent"
  app:layout_constraintRight_toRightOf="parent"
  app:layout_constraintLeft_toLeftOf="parent"
  android:transitionName="@string/tink_toolbar_transition"
  app:buttonGravity="center_vertical"
  app:navigationContentDescription="@string/tink_nav_app_bar_navigate_up_description"
  app:navigationIcon="?homeAsUpIndicator" />

<include
  android:id="@+id/loader"
  layout="@layout/tink_loader_spinner"
  app:layout_constraintTop_toTopOf="parent"
  app:layout_constraintBottom_toBottomOf="parent'"
  app:layout_constraintStart_toStartOf="parent"
  app:layout_constraintEnd_toEndOf="parent"
  />

<LinearLayout
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:orientation="vertical"
  app:layout_constraintBottom_toBottomOf="parent"
  app:layout_constraintLeft_toLeftOf="parent"
  app:layout_constraintRight_toRightOf="parent"
  app:layout_constraintTop_toBottomOf="@+id/tink_toolbar">

    <Button
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="123" />

</LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>


